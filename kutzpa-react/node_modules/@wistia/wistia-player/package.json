{
  "name": "@wistia/wistia-player",
  "version": "0.0.108",
  "packageManager": "yarn@4.5.3",
  "description": "An embeddable wistia-player web component and React wrapper to add responsive, lightweight, and SEO-friendly videos to your site.",
  "keywords": [
    "wistia",
    "video",
    "player",
    "embed",
    "web component",
    "custom element"
  ],
  "license": "UNLICENSED",
  "author": "Wistia Engineering",
  "main": "./dist/wistia-player.js",
  "module": "./dist/wistia-player.js",
  "types": "./dist/types/embeds/wistiaPlayer/WistiaPlayer.d.ts",
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:player:dev": "webpack --config ./webpack.config.cjs",
    "build:player:prod": "NODE_ENV=production webpack --config ./webpack.config.cjs",
    "build:types:wistia-player": "tsc --project ../../src/embeds/wistiaPlayer/tsconfig.json",
    "prebuild": "rm -rf dist",
    "build:dev": "npm-run-all 'prebuild' 'build:player:dev' 'build:types:wistia-player'",
    "build:prod": "npm-run-all 'prebuild' 'build:player:prod' 'build:types:wistia-player'",
    "start:dev": "yarn run build:player:dev --watch",
    "start:dev:yalc": "yarn run build:player:dev --env yalc=true --watch "
  },
  "dependencies": {
    "dotenv-webpack": "^8.1.0",
    "iso8601-duration": "^2.1.2",
    "lodash.merge": "^4.6.2",
    "mux-embed": "^5.9.0",
    "preact": "10.26.5"
  },
  "devDependencies": {
    "@babel/core": "^7.26.9",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/preset-env": "^7.26.9",
    "@babel/preset-react": "^7.26.3",
    "@babel/preset-typescript": "^7.26.0",
    "babel-loader": "^10.0.0",
    "babel-plugin-transform-object-hasown": "^1.1.0",
    "coffee-loader": "^5.0.0",
    "coffeescript": "1.12.7",
    "compression-webpack-plugin": "^11.1.0",
    "npm-run-all": "^4.1.5",
    "read-package-json": "^7.0.1",
    "typescript": "5.7.3",
    "webpack": "^5.99.5",
    "webpack-cli": "^6.0.1",
    "webpack-merge": "^6.0.1",
    "webpack-shell-plugin-next": "^2.3.2"
  }
}
